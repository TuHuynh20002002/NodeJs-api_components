generator client {
  provider = "prisma-client-js"
  previewFeatures = ["omitApi"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  email    String  @unique(map: "email_1")
  password String?
  position String?
  username String  @unique(map: "username_1")
}
